{
    "python.defaultInterpreterPath": "./env/Scripts/python.exe",
    "python.formatting.provider": "black",
    "python.formatting.blackArgs": [
        "-S"
    ],
    "python.analysis.extraPaths": [
        "./apps/"
    ],
    "files.exclude": {
        "**/*.pyc": true,
        "**/__pycache__": true,
    },
}